<html>
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
</head>
<body>
	<div class="panel-group">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" href="#collapse1">Monthly Timesheet Reports for Finance</a>
        </h4>
        <div id="collapse1" class="panel-collapse collapse">
          <%= form_tag({:action => 'datewise_reports',  method: "post"}) do %>
            <%= render 'form_date'%>
            <%= submit_tag("View Report") %>
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <div class="panel-group">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" href="#collapse2">Not Approving By Manager</a>
        </h4>
        <div id="collapse2" class="panel-collapse collapse">
          <%= form_tag({:action => 'not_approved',  method: "post"}) do %>
            <%= render 'form_date'%>
            <%= submit_tag("View not Approving Report") %>
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <div class="panel-group">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" href="#collapse3">Defaulters</a>
        </h4>
        <div id="collapse3" class="panel-collapse collapse">
          <%= form_tag({:action => 'defaulters_report',  method: "post"}) do %>
            <%= render 'form_date'%>
            <%= submit_tag("View Defaulters Report") %>
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <div class="panel-group">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" href="#collapse4">Report for Managers</a>
        </h4>
        <div id="collapse4" class="panel-collapse collapse">
          <%= form_tag({:action => 'managers_report',  method: "post"}) do %>
            <%= render 'form_date'%>
            <%= submit_tag("View Managers Report") %>
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <div class="panel-group">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" href="#collapse5">Active Projects</a>
        </h4>
        <div id="collapse5" class="panel-collapse collapse">
          <%= form_tag({:action => 'active_projects_report',  method: "post"}) do %>
            <% render 'form_date'%>
            <%= submit_tag("All Projects") %>
          <% end %>
        </div>
      </div>
    </div>
  </div>

</body>
</html>